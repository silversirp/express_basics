<h1>Questions about Express</h1>
<% questions.forEach((question)=> { %>
    <% if(question['user'].toLowerCase() === 'kadi') { %>
        <h2> <%= question['title'] %></h2>
        <p> <%= question['user'] %></p>
        <p> <%= question['votes'] %></p>
<% } else { %>
    <h1>Asked By Other Users:</h1>
    <h2> <%= question['title'] %></h2>
    <p> <%= question['user'] %></p>
    <p> <%= question['votes'] %></p>
<% } %>
<% }); %>